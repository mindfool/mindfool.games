{
  "expo": {
    "name": "MindFool.Games",
    "slug": "mindfool-games",
    "version": "0.1.0",
    "scheme": "mindfool",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#F5F7FA"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.mindfool.games"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#F5F7FA"
      },
      "package": "com.mindfool.games",
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "web": {
      "bundler": "metro",
      "favicon": "./assets/icon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "9808776e-69d9-45a0-8d3c-3fd88c87eba3"
      }
    }
  }
}
